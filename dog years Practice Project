// This is the variable for my Age:
const myAge = 25;
//This represents the first 2 years of a dog's life:
let earlyYears = 2;
earlyYears = earlyYears*10.5;
//This variable takes into account the first 2 years of my Life:
let laterYears = (myAge - 2)*4;
//Checking my work:
console.log(earlyYears);
console.log(laterYears);
//This is my age in dog years:
const myAgeInDogYears = earlyYears + laterYears;
// This is my name in all Lower Case:
let myName = "Benny Louie".toLowerCase();
console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)
